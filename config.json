{
	"source": [ "token.json" ],
	"platforms": {
		"scss": {
			"transformGroup": "scss",
			"buildPath": "build/scss/",
			"files": [
				{
					"destination": "_variables.scss",
					"format": "scss/variables"
				}
			]
		},
		"android": {
			"basePxFontSize": 1,
			"transformGroup": "android",
			"buildPath": "build/android/",
			"files": [
				{
					"destination": "all.xml",
					"format": "android/resources"
				},
				{
					"destination": "colors.xml",
					"format": "android/colors"
				},
				{
					"destination": "font_dimens.xml",
					"format": "android/fontDimens"
				},
        {
					"destination": "border_radius.xml",
					"format": "android/dimens",
					"filter": {
						"attributes": {
							"category": "borderRadius"
						}
					}
				},
				{
					"destination": "size.xml",
					"format": "android/dimens",
					"filter": {
						"attributes": {
							"category": "size"
						}
					}
				},
				{
					"destination": "integers.xml",
					"format": "android/integers"
				}
			]
		},
		"compose": {
			"basePxFontSize": 1,
			"transformGroup": "compose",
			"buildPath": "build/compose/",
			"files": [
				{
					"destination": "Color.kt",
					"format": "compose/object",
					"className": "Color",
					"packageName": "Color",
					"filter": {
						"attributes": {
							"category": "color"
						}
					}
				},
        {
					"destination": "BorderRadius.kt",
					"format": "compose/object",
					"className": "BorderRadius",
					"packageName": "BorderRadius",
					"filter": {
						"attributes": {
							"category": "borderRadius"
						}
					}
				},
        {
					"destination": "Icons.kt",
					"format": "compose/object",
					"className": "Icons",
					"packageName": "Icons",
					"filter": {
						"attributes": {
							"category": "icons"
						}
					}
				},
        {
					"destination": "Spacing.kt",
					"format": "compose/object",
					"className": "Spacing",
					"packageName": "Spacing",
					"filter": {
						"attributes": {
							"category": "spacing"
						}
					}
				},
        {
					"destination": "ComponentStates.kt",
					"format": "compose/object",
					"className": "ComponentStates",
					"packageName": "ComponentStates",
					"filter": {
						"attributes": {
							"category": "componentStates"
						}
					}
				},
				{
					"destination": "typography.kt",
					"format": "compose/object",
					"className": "Typography",
					"packageName": "Typography",
					"filter": {
						"attributes": {
							"category": "Typography"
						}
					}
				},
        {
					"destination": "Opacity.kt",
					"format": "compose/object",
					"className": "Opacity",
					"packageName": "Opacity",
					"filter": {
						"attributes": {
							"category": "opacity"
						}
					}
				},
				{
					"destination": "Size.kt",
					"format": "compose/object",
					"className": "Size",
					"packageName": "Size",
					"filter": {
						"attributes": {
							"category": "size"
						}
					}
				},
				{
					"destination": "All.kt",
					"format": "compose/object"
				}
			]
		},
		"ios": {
			"transformGroup": "ios",
			"buildPath": "build/ios/",
			"files": [
				{
					"destination": "StyleDictionaryColor.h",
					"format": "ios/colors.h",
					"className": "StyleDictionaryColor",
					"type": "StyleDictionaryColorName",
					"filter": {
						"attributes": {
							"category": "color"
						}
					}
				},
				{
					"destination": "StyleDictionaryColor.m",
					"format": "ios/colors.m",
					"className": "StyleDictionaryColor",
					"type": "StyleDictionaryColorName",
					"filter": {
						"attributes": {
							"category": "color"
						}
					}
				},
				{
					"destination": "StyleDictionarySize.h",
					"format": "ios/static.h",
					"className": "StyleDictionarySize",
					"type": "float",
					"filter": {
						"attributes": {
							"category": "size"
						}
					}
				},
				{
					"destination": "StyleDictionarySize.m",
					"format": "ios/static.m",
					"className": "StyleDictionarySize",
					"type": "float",
					"filter": {
						"attributes": {
							"category": "size"
						}
					}
				}
			]
		},
		"ios-swift": {
			"transformGroup": "ios-swift",
			"buildPath": "build/ios-swift/",
			"files": [
				{
					"destination": "StyleDictionary+Class.swift",
					"format": "ios-swift/class.swift",
					"className": "StyleDictionaryClass",
					"filter": {}
				},
				{
					"destination": "StyleDictionary+Enum.swift",
					"format": "ios-swift/enum.swift",
					"className": "StyleDictionaryEnum",
					"filter": {}
				},
				{
					"destination": "StyleDictionary+Struct.swift",
					"format": "ios-swift/any.swift",
					"className": "StyleDictionaryStruct",
					"filter": {},
					"options": {
						"imports": "SwiftUI",
						"objectType": "struct",
						"accessControl": "internal"
					}
				}
			]
		},
		"ios-swift-separate-enums": {
			"transformGroup": "ios-swift-separate",
			"buildPath": "build/ios-swift/",
			"files": [
				{
					"destination": "StyleDictionaryColor.swift",
					"format": "ios-swift/enum.swift",
					"className": "StyleDictionaryColor",
					"filter": {
						"attributes": {
							"category": "color"
						}
					}
				},
				{
					"destination": "StyleDictionarySize.swift",
					"format": "ios-swift/enum.swift",
					"className": "StyleDictionarySize",
					"filter": {
						"attributes": {
							"category": "size"
						}
					}
				}
			]
		}
	}
}
